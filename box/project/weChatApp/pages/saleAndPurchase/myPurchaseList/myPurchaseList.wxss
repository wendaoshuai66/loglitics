  /* background: red; */
  border-radius: 50%;
  /* animation: redAnimation 2s; */
}

@keyframes redAnimation {
  0% {
    box-shadow: 0px 0px 5rpx red;
  }

  50% {
    box-shadow: 0px 0px 15rpx red;
  }

  100% {
    box-shadow: 0px 0px 5rpx red;
  }
}

@keyframes lightgreyAnimation {
  0% {
    box-shadow: 0px 0px 5rpx lightgrey;
  }

  50% {
    box-shadow: 0px 0px 15rpx lightgrey;
  }

  100% {
    box-shadow: 0px 0px 5rpx lightgrey;
  }
}

@keyframes greenAnimation {
  0% {
    box-shadow: 0px 0px 5rpx green;
  }

  50% {
    box-shadow: 0px 0px 15rpx green;
  }

  100% {
    box-shadow: 0px 0px 5rpx green;
  }
}

