.dealButton image {
  width: 100rpx;
  height: 100rpx;
  background: transparent;
}

.centerImage {
  width: 110rpx;
  height: 90rpx;
  position: absolute;
  top: 76%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 999;
  opacity: 0.5;
}

.dealText {
  font-size: 25rpx;
  color: #999;
}

button[size="mini"] {
  padding: 0 20rpx !important;
  line-height: 48rpx;
  background: transparent;
  color: #F36A5A;
  border: 1px solid #F36A5A;
  border-radius: 30rpx;
  font-size: 26rpx;
}

button[disabled][type="default"], wx-button[disabled]:not([type]) {
  background: transparent;
  color: #888;
  border: 1px solid #ccc;
  font-size: 26rpx;
  border-radius: 30rpx;
}

.sale-title {
  border-radius: 20rpx;
  color: #fff;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  padding: 8rpx 24rpx;
  margin: 5rpx 10rpx 0 0;
  display: inline-block;
}
.lightgrey{
  background:	lightgrey;
  animation:lightgreyAnimation 2s;
  animation-iteration-count: infinite;
}
.red{
  background:red;
  animation:redAnimation 2s;
  animation-iteration-count: infinite;
}
.complete{
  color: green !important;
  border: 1px solid green !important;
}
