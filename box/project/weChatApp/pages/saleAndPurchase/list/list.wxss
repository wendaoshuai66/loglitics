

page {
  background-color: #fff !important;
}

.wxSearch-section {
  height: 40px;
  z-index: 20000;
}
.weui-icon-search_in-box{
  position:absolute;
  left:10px;
  top:10px;
}
.cancel{
  position: absolute;
  top: 50%;
  right: 40rpx;
  transform: translateY(-50%);
  z-index: 10000000000000000000;
}
.searchInput{
  padding-left: 30px;
  font-size: 27rpx;
  line-height: 45rpx;
  height: 31px;
  width: 70%;
}
.mask{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100px; 
  background: black;
  opacity: 0.7;
  z-index: 1;
  transform: translateY(100%);
}

.logBox{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  /* height: 500rpx; */
  background: white;
  transform: translateY(100%);
  z-index:1000000000001;
  display: inline-block;
}
.logText{
  text-align: center;
  color: #dedede;
  font-size: 30rpx;
}
.logItemBox{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: #999;
  border-bottom: 1px solid #e5e5e5;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  /* border-top: 1px solid #ccc; */
}
.searchBox{
  width: 94%;
  height: 70%;
  /* padding-left: 25rpx; */
  background: white;
  position: absolute;
  border-radius: 5px;
  top: 50%;
  /* left: 50%; */
  left: 25rpx;
  transform: translate(-0%,-50%)
}
.logBottom{
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 14rpx 0rpx;
  /* border-bottom: 1px solid #ccc; */
}
.scrollView {
  width: 100%;
  height: 100%;
  flex: 1;
}

.text {
  justify-content: center;
  align-items: center;
  float: right;
  width: 100%;
}

.title {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
}

.item {
  margin: 20rpx 28rpx;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  flex-direction: row;
  position: relative;
}

.allDownText {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
  margin-bottom: 20rpx;
  color: #888;
  font-size: 30rpx;
  margin-top:12rpx;
}

.wxSearch {
  z-index: 19999;
}

page {
  background: #999;
  display: flex;
  width: 100%;
}

.MainContent {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}

.TopBlock {
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  position: fixed;
  height: 45px;
  z-index: 19999;
  background: #e5e5e5;
}

.BottomBlock {
  margin-top: 100px;
  display: flex;
  flex: 1;
  overflow: auto;
}

.searchBar {
  width: 100%;
  margin-top: 45px;
  position: fixed;
  z-index: 18888;
  background-color: #f5f5f5;
}

.modelPicker {
  width: 100%;
  margin-top: 10px;
  position: fixed;
  z-index: 18888;
}

.datePickerStart {
  width: 100%;
  margin-top: 40px;
  position: fixed;
  z-index: 18888;
}

.datePickerEnd {
  width: 100%;
  margin-top: 70px;
  position: fixed;
  z-index: 18888;
}

.groupButton {
  width: 100%;
  margin-top: 100px;
  position: fixed;
  z-index: 18888;
}

.picker {
  padding: 0px 15px;
  font-size: 15px;
}

.radios {
  width: 43%;
  float: left;
  padding: 0 0 0 15px;
  font-size: 15px;
}

.label {
  margin-right: 15rpx;
}

.input_content {
  width: 47%;
  height: 100%;
  float: left;
}

.resetButton {
  margin-left: 20px;
  width: 40%;
  height: 45px;
}

.seachButton {
  position: fixed;
  width: 40%;
  height: 45px;
  margin-top: -45px;
}

.picture {
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
  border: 1px solid #ccc;
  padding: 8rpx;
}

.buy-title {
  font-size: 30rpx;
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  line-height: 44rpx;
}

.buy-price {
  font-size: 24rpx;
  color: #666;
  width: 564rpx;
  margin-top: 40rpx;
  line-height: 12rpx;
}

.publisher {
  float: left;
}

.price {
  float: left;
}

.buy-time {
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  width: 100%;
  margin-bottom: 14rpx;
  line-height: 36rpx;
}

.time {
  clear: right;
  float: right;
}

.nav {
  width: 100%;
  position: relative;
}

.flagType {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 25%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.flagTime {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 75%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.nav_tap {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  box-sizing: border-box;
  border-left: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  text-align: center;
  font-size: 30rpx;
  display: inline-block;
  background: #f5f5f5;
  position: relative;
  /* z-index: 111111; */
}

.typeList {
  /* width: 100%; */
  width: 200%;
  height: 100rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 0;
  line-height: 100rpx;
  text-align: left;
  z-index: 1000;
}

.typeList text {
  padding: 10rpx 20rpx;
  background: #dedede;
  /* float: left; */
  margin-left: 30rpx;
  border-radius: 10rpx;
  font-size: 25rpx;
}

.timeList {
  width: 200%;
  height: 100rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: -100%;
  line-height: 100rpx;
  text-align: left;
  z-index: 1000;
}

.timeList text {
  padding: 10rpx 20rpx;
  background: #dedede;
  /* float: left; */
  margin-left: 30rpx;
  border-radius: 10rpx;
  font-size: 25rpx;
}

.statusN {
  position: absolute;
  top: 50rpx;
  right: 0;
  opacity: 0.5;
  padding: 4rpx 14rpx;
  font-size: 24rpx;
  color: red;
  border: 1px solid red;
  transform: rotate(-20deg);
}

.statusY {
  position: absolute;
  bottom: 78rpx;
  right: 0;
  opacity: 0.5;
  padding: 4rpx 14rpx;
  font-size: 24rpx;
  color: green;
  border: 1px solid green;
  transform: rotate(-20deg);
}

.li {
  display: inline-block;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  width: 50%;
  box-sizing: border-box;
  background: #f5f5f5;
  position: relative;
  font-size: 32rpx;
  z-index: 100;
  color: #3cafd9;
}

.typeTitle {
  border-right: 1px solid #e5e5e5;
}

.ul {
  position: relative;
}

.titleList {
  position: absolute;
  top: -400rpx;
  left: 0;
  width: 100%;
  /* transform:translateY(100%); */
  background-color:#ebebec;
  text-align: left;
  z-index: 1;
  font-size: 30rpx;
  color: #888;
}

.titleList view {
  border-bottom: 1px solid #dbdbdb;
  text-indent: 32rpx;
  padding: 20rpx 0;
}

.searchBar {
  width: 100%;
  margin-top: 45px;
  position: fixed;
  height: 100rpx;
  z-index: 18888;
  background-color: #eee;
}

.sale-title {
  border-radius: 20rpx;
  color: #fff;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  padding: 8rpx 24rpx;
  margin: 5rpx 10rpx 0 0;
  display: inline-block;
  background: #8ad0ff;
}

/* 小图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-all:before {
  content: "\e600";
}
