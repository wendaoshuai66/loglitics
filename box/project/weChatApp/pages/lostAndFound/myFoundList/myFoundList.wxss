}
@keyframes redAnimation{
  0%{box-shadow: 0px 0px 5rpx red;}
  50%{box-shadow: 0px 0px 15rpx red;}
  100%{box-shadow: 0px 0px 5rpx red;}
}
@keyframes lightgreyAnimation {
  0% {
    box-shadow: 0px 0px 5rpx lightgrey;
  }

  50% {
    box-shadow: 0px 0px 15rpx lightgrey;
  }

  100% {
    box-shadow: 0px 0px 5rpx lightgrey;
  }
}
@keyframes greenAnimation{
  0%{box-shadow: 0px 0px 5rpx green;}
  50%{box-shadow: 0px 0px 15rpx green;}
  100%{box-shadow: 0px 0px 5rpx green;}
}
button[size="mini"] {
  padding: 0 20rpx !important;
  line-height: 48rpx;
  background: transparent;
  color: #F36A5A;
  border: 1px solid #F36A5A;
  border-radius: 30rpx;
  margin-top:16rpx;
}

button[disabled][type="default"], wx-button[disabled]:not([type]) {
  background: transparent;
  color: #888;
  border: 1px solid #ccc;
  font-size: 26rpx;
  border-radius: 30rpx;
}
.lightgrey{
  background:	lightgrey;
  animation:lightgreyAnimation 2s;
  animation-iteration-count: infinite;
}
.red{
  background:red;
  animation:redAnimation 2s;
  animation-iteration-count: infinite;
}
.complete{
  color: green !important;
  border: 1px solid green !important;
}
