/* pages/statistics/list/list.wxss */

.body {
  padding: 40rpx;
}

.title {
  padding: 10rpx 32rpx;
  height: 100rpx;
  line-height: 104rpx;
  border-radius: 16rpx;
  color: #fff;
  margin: 58rpx 0;
  font-size:32rpx;
}

.content {
  min-height: 600rpx;
  background: #f5f5f5;
  padding: 20rpx;
  border-radius: 12rpx;
  box-shadow: 5rpx 5rpx 50rpx #c0c1c1;
}

.time-sharing {
  background: #5497e4;
}

.maintenance-capacity {
  background: #98cd5a;
}

.maintenance-quality {
  background: #33bbaf;
}

.li {
  background: white;
  border-bottom: 1px solid #e5e5e5;
  padding: 20rpx 80rpx;
  color: #888;
  font-size: 32rpx;
}

.arrow {
  display: inline-block;
  float: right;
}

.arrow image {
  width: 24rpx;
  height: 40rpx;
  vertical-align: middle;
}

.toTest {
  background: blueviolet;
  padding: 10rpx 30rpx;
}

/* 小图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 56rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  display:inline-block;
  vertical-align:bottom;
  margin-right: 12rpx;
}

.icon-timer1:before {
  content: "\e61c";
}

.icon-capacity:before {
  content: "\e603";
}

.icon-site-quality:before {
  content: "\e604";
}
