/*小图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  text-align: center;
  width: 160rpx;
  height: 160rpx;
  line-height: 160rpx;
  border-radius: 40rpx;
  font-size: 88rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-news:before {
  content: "\e62e";
  color: #fff;
}

.icon-balance:before {
  content: "\e634";
  color: #fff;
}

.icon-voice:before {
  content: "\e662";
  color: #fff;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.latest-information {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-icon_basicinformation:before {
  content: "\e60b";
}

.icon-Repairapplication:before {
  content: "\e62e";
}

.bottomBlock {
  margin-top: 20rpx;
  background: white;
  padding: 0rpx 24rpx;
  /* border-top:1px solid #ccc; */
}

.list {
  border-bottom: 1px solid #e5e5e5;
  overflow: hidden;
  padding: 20rpx 0rpx;
  position: relative;
  width: 100%;
  box-sizing: border-box;
}

.picture {
  width: 35%;
  height: 176rpx;
  float: left;
  padding: 0rpx 16rpx 0 0;
  box-sizing: border-box;
}

.title {
  font-size: 28rpx;
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.dateTime {
  float: right;
  color: #888;
  font-size: 26rpx;
  padding-right: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.author {
  float: right;
  color: #888;
  font-size: 26rpx;
  padding-right: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 140rpx;
  overflow: hidden;
}

.name {
  color: #888;
  font-size: 26rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 148rpx;
}

.bottom {
  width: 65%;
  position: absolute;
  right: 0rpx;
  margin-top: 4rpx;
}

.listTitle .text {
  font-size: 30rpx;
  font-weight: bold;
  background: skyblue;
  position: absolute;
  top: 50%;
  left: 40rpx;
  transform: translate(0%, -50%);
  border-radius: 50rpx;
  padding: 10rpx 25rpx;
  color: white;
}

.text {
  text-align: center;
  border-bottom: 2px solid #12a3c0;
  font-size: 36rpx;
  color: #12a3c0;
  height: 80rpx;
  line-height: 80rpx;
}

.listTitle {
  width: 100%;
  height: 28rpx;
  position: relative;
  margin-top: 50rpx;
  background: #f0f0f0;
}

.bottomMessage {
  font-size: 30rpx;
  text-align: center;
  color: #666;
}

.noRoleClass {
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  font-size: 70rpx;
  margin: 30rpx 0rpx;
  display: inline-block;
  margin-bottom: 25rpx;
}

.mantenanceClass {
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 55rpx;
  margin: 30rpx 0rpx;
  display: inline-block;
  margin-bottom: 25rpx;
}

.inforText {
  font-size: 28rpx;
}

.maintenanceInfor {
  width: 25%;
  display: inline-block;
  box-sizing: border-box;
  text-align: center;
}

