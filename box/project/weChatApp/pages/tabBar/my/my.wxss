.warn_content {
  width: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: #999;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin: 38rpx 20rpx 20rpx 20rpx;
  border-radius: 70%;
}

.userinfo-nickname {
  color: #333;
}

.usermotto {
  margin-top: 100rpx;
}

.info-hd {
  overflow: hidden;
  transition: 0.2s;
}

.btn-open {
  text-align: left;
  height: 100rpx;
  line-height: 80rpx;
  color: #666;
  font-size: 32rpx;
  border-bottom: 1px solid #eaecee;
  /* background-color: #e1edf8; */
  margin:0rpx 20rpx 20rpx 20rpx;
  /* overflow: hidden; */
}

.pdleft {
  padding-left: 100rpx;
}

.enter {
  width: 24rpx;
  height: 40rpx;
  float: right;
  line-height: 20rpx;
  margin: 22rpx 40rpx 0 0;
  /* transform: rotate(90deg)  */
  transition: 0.2s;
}

.enterTap {
  width: 24rpx;
  height: 40rpx;
  float: right;
  line-height: 20rpx;
  margin: 28rpx 40rpx 0 0;
  transform: rotate(90deg);
  transition: 0.2s;
}

 .repair {
  margin-top: 20rpx;
}

