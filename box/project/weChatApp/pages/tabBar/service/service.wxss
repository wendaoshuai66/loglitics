.warn_content {
  width: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: #999;
}

.service-photo {
  width: 100%;
  height: 440rpx !important;
}

.container-photo {
  height: 440rpx;
}

.enter {
  width: 24rpx;
  height: 40rpx;
  float: right;
  line-height: 40rpx;
  margin: 20rpx 40rpx 0 0;
}

.service-category {
  text-align: left;
  height: 110rpx;
  line-height: 78rpx;
  text-indent: 20rpx;
  color: #666;
  font-size: 36rpx;
  border-bottom: 1px solid #eaecee;
  margin: 32rpx 20rpx;
  letter-spacing: 4rpx;
}

