.span {
  text-align: center;
  color: #666;
  font-size: 30rpx;
}

.list {
  margin-top: 32rpx;
  /* margin-bottom: 20rpx; */
  background: white;
  overflow: hidden;
  border-bottom: 1px solid #e5e5e5;
  position: relative;
}

.datetime {
  float: right;
  font-size: 24rpx;
  color: #999;
  margin-top: 14rpx;
}

.approvalImage {
  width: 80rpx;
  height: 70rpx;
  opacity: 0.5;
  margin: 0 0 0rpx 20rpx;
}

.replyTitle {
  color: #4ba634;
}

/* 小图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  font-size: 34rpx;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  margin-right: 8rpx;
}

.icon-_ico_reply:before {
  content: "\e6df";
  color: #2ba245;
}

page {
  background: #f5f5f5;
}

.reply {
  left: 0rpx;
  font-size: 25rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  margin-bottom: 16rpx;
}

.reply_bottom {
  font-size: 24rpx;
}

.replyTitle {
  color: #4ba634;
}

.headPicture {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 10rpx;
}

.content_body {
  margin-top: 100rpx;
}

.allDownText {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
  margin-bottom: 20rpx;
  color: #888;
  font-size: 30rpx;
}

.item {
  width: 95%;
  margin: 28rpx auto;
  background: white;
  padding: 20rpx 20rpx 4rpx 20rpx;
  box-sizing: border-box;
  box-shadow: 0px 0px 15rpx #ccc;
}

.title {
  text-align: left;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-bottom: 10rpx;
  border-left: 10rpx solid #1ba39c;
  padding-left: 20rpx;
  white-space: nowrap;
  font-size: 30rpx;
  color: #333;
  /* text-overflow: hidden; */
}

.content {
  font-size: 28rpx;
  color: #666;
  /* background: red; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 8rpx;
}

.name {
  font-size: 28rpx;
  color: #999;
}

.inline-block {
  display: inline-block;
}

.content_bottom {
  border-top: 1px solid #e5e5e5;
  padding-top: 18rpx;
  color: #e5e5e5;
  font-size: 30rpx;
}

.date {
  float: right;
  font-size: 24rpx;
  margin-top: 12rpx;
  color: #999;
}
