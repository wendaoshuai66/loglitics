/* @import "../../../wxSearch/wxSearch.wxss"; */

page {
  background-color: #e5e5e5 !important;
}
.showPage{
  height: 100%;
  width: 100%;
  background: white; 
  position: fixed;
  z-index: 1000;
  top: 0;
  left: 0;
}

.weui-icon-search_in-box{
  position:absolute;
  left:10px;
  top:10px;
}
.searchInput{
  padding-left: 30px;
  font-size: 27rpx;
  line-height: 45rpx;
  height: 31px;
}
.mask{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100px; 
  background: black;
  opacity: 0.7;
  z-index: 1;
  transform: translateY(100%);
}

.logBox{
  display:none;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  /* height: 500rpx; */
  background: white;
  transform: translateY(100%);
  z-index:1000000000001;
  /* display: inline-block; */
}
.logText{
  text-align: center;
  color: #dedede;
  font-size: 30rpx;
}
.logItemBox{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: #999;
  border-bottom: 1px solid #e5e5e5;
  height: 100rpx;
  line-height: 100rpx;
  /* border-top: 1px solid #ccc; */
}
.logBottom{
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 20rpx 0rpx;
  /* border-bottom: 1px solid #ccc; */
}
.title {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
}
.searchBox{
  width: 94%;
  height: 70%;
  background: white;
  position: absolute;
  border-radius: 5px;
  top: 50%;
  left: 25rpx;
  transform: translate(0%,-50%)
}
.item {
   /* height: 208rpx;  */
  margin: 20rpx 0rpx;
  display: flex;
  flex-direction: row;
  background: white; 
  position: relative;
  padding:20rpx 10rpx;
  border-radius: 6rpx;
}

.allDownText {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
  margin-bottom: 20rpx;
  color: #888;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.wxSearch {
  z-index: 19999;
}

page {
  background: #999;
  display: flex;
  width: 100%;
  overflow: auto;  
}

.MainContent {
  position: relative;
  /* height: 500rpx; */
  width: 100%;
  /* overflow: auto; */
}

 .TopBlock {
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  position: fixed;
  height: 92rpx;
  z-index: 19999;
  background: #dedede;
} 

.BottomBlock {
  margin-top: 180rpx;
  /* display: flex;
  flex: 1;
  overflow: auto; */
}

.searchBar {
  width: 100%;
  margin-top: 92rpx;
  position: fixed;
  z-index: 18888;
  background-color: #f5f5f5;
}

.modelPicker {
  width: 100%;
  margin-top: 10px;
  position: fixed;
  z-index: 18888;
}

.datePickerStart {
  width: 100%;
  margin-top: 40px;
  position: fixed;
  z-index: 18888;
}

.datePickerEnd {
  width: 100%;
  margin-top: 70px;
  position: fixed;
  z-index: 18888;
}

.groupButton {
  width: 100%;
  margin-top: 100px;
  position: fixed;
  z-index: 18888;
}

.picker {
  padding: 0px 15px;
  font-size: 15px;
}

.radios {
  width: 43%;
  float: left;
  padding: 0 0 0 15px;
  font-size: 15px;
}

.label {
  margin-right: 15rpx;
}

.input_content {
  width: 47%;
  height: 100%;
  float: left;
}

.resetButton {
  margin-left: 20px;
  width: 40%;
  height: 45px;
}

.seachButton {
  position: fixed;
  width: 40%;
  height: 45px;
  margin-top: -45px;
}

.picture {
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
}

.text {
  justify-content: center;
  align-items: center;
  float: right;
  width: 100%;
}

.buy-title {
  font-size: 32rpx;
  color: #333;
  line-height: 44rpx;
  text-align: left;
  margin-left: 52rpx;
  margin-bottom: 8rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.buy-price {
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  width: 564rpx;
  margin-top: 74rpx;
  line-height: 12rpx;
}

.publisher {
  float: left;
}

.price {
  float: left;
}

.content-text{
  font-size: 28rpx;
  color: #999;
  margin-left: 52rpx;
}
.content-title{
color:#666;
}
.buy-time {
  border-top: 1px solid  #eaecee;
  padding-top: 10rpx;
  font-size: 26rpx;
  display: inline-block;
  width: 100%;
  /* margin-top: 10rpx; */
  margin-bottom: 8rpx;
  line-height: 36rpx;
}
.numbers{
  color: #666;
  margin-left: 48rpx;
}
.time {
  clear: right;
  float: right;
  font-size: 26rpx;
  color:#999;
}

.nav {
  width: 100%;
  position: relative;
}

.flagType {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 25%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.flagTime {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 75%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.nav_tap {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  box-sizing: border-box;
  border-left: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  text-align: center;
  font-size: 30rpx;
  display: inline-block;
  background: #f5f5f5;
  position: relative;
}

.ul {
  position: relative;
}

.typeTitle {
  border-right: 1px solid #e5e5e5;
}

.titleList {
  position: absolute;
  top: 100% !important;
  left: 0;
  width: 100%;
  /* transform:translateY(100%); */
  text-align: left;
  z-index: 1;
  font-size: 30rpx;
  color: #888;
  /* height: 200px; */
  overflow-y: scroll;
  background-color:#ebebec;
}

.titleList view {
  border-bottom: 1px solid #dbdbdb;
  text-indent: 32rpx;
  padding: 20rpx 0;
}

.li {
  display: inline-block;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  width: 50%;
  box-sizing: border-box;
  background: #f5f5f5;
  position: relative;
  z-index: 100;
  font-size: 32rpx;
  color: #24a99b;
}

/* 小图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-all:before {
  content: "\e600";
}
.maintenanceStatus{
  position: absolute;
  top: 0; 
  font-size: 26rpx;
  width: 40rpx;
  text-align: center;
  height: 188rpx;
  color: white;
  padding-top: 14rpx;
  line-height: 32rpx;
  background: #1BA39C;
  /* box-shadow: 0rpx -10rpx 10px black */
}
.relate{
  width: 100%;
  height: 100%;
  position: relative;
}
.before{
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 30rpx;
  height: 30rpx;
  background: white;
  transform: translate(-50%,50%) rotate(45deg);

}


