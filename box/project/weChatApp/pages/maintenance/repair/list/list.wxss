.body {
  margin-top: 82rpx;
  overflow: hidden;
}

.span {
  margin-top: 100rpx;
}

.list {
  margin: 0 auto;
  margin-top: 20rpx;
  width: 90%;
  border-radius: 5rpx;
  overflow: hidden;
  background: white;
  padding-bottom: 50rpx;
  box-shadow: 0px 0px 50rpx #dedede;
  position: relative;
}

page {
  background: #f5f5f5;
}

.allDownText {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right: 15rpx;
  margin-bottom: 20rpx;
  color: #888;
  font-size: 30rpx;
}

.statuG {
  position: absolute;
  bottom: 100rpx;
  right: 20rpx;
  border: 1px solid green;
  font-size: 30rpx;
  padding: 0 20rpx;
  color: green;
  transform: rotate(25deg);
  opacity: 0.5;
}

.statuR {
  position: absolute;
  bottom: 100rpx;
  right: 20rpx;
  border: 1px solid red;
  font-size: 30rpx;
  padding: 0 20rpx;
  color: red;
  transform: rotate(25deg);
  opacity: 0.5;
}

.title {
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
  color: #000;
  font-size: 35rpx;
  font-weight: bold;
}

.text {
  width: 100%;
}

.headText {
  padding: 0 20rpx;
}

.headBox {
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
}

.author {
  float: left;
  font-size: 35rpx;
}

.tel {
  padding: 0 10rpx;
  display: inline;
  border: 2px solid #1c87f4;
  color: #1c87f4;
  border-radius: 15rpx;
  font-size: 33rpx;
}

.address {
  border-bottom: 1px solid #dedede;
  font-size: 33rpx;
  padding-bottom: 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.content {
  padding: 0 20rpx;
  font-size: 30rpx;
}

.li {
  margin-top: 20rpx;
  /* 超出一行显示省略号 */
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.span {
  /* background: #dedede; */
  text-align: center;
  font-size: 30rpx;
  color: #666;
}

.item {
  /* height: 208rpx;  */
  margin: 20rpx 0rpx;
  display: flex;
  flex-direction: row;
  background: white;
  position: relative;
  padding: 20rpx 10rpx;
  border-radius: 6rpx;
}

.buy-title {
  font-size: 32rpx;
  color: #333;
  line-height: 44rpx;
  text-align: left;
  margin-left: 52rpx;
  margin-bottom: 8rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.buy-price {
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  width: 564rpx;
  margin-top: 74rpx;
  line-height: 12rpx;
}

.publisher {
  float: left;
}

.price {
  float: left;
}

.content-text {
  font-size: 28rpx;
  color: #999;
  margin-left: 52rpx;
}

.content-title {
  color: #666;
}

.buy-time {
  border-top: 1px solid #eaecee;
  padding-top: 10rpx;
  font-size: 26rpx;
  display: inline-block;
  width: 100%;
  /* margin-top: 10rpx; */
  margin-bottom: 8rpx;
  line-height: 36rpx;
}

.numbers {
  color: #666;
  margin-left: 48rpx;
}

.time {
  clear: right;
  float: right;
  font-size: 26rpx;
  color: #999;
}

.nav {
  width: 100%;
  position: relative;
}

.flagType {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 25%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.flagTime {
  width: 30rpx;
  height: 30rpx;
  background: skyblue;
  position: absolute;
  top: 100%;
  left: 75%;
  transform: translate(-50%, -50%) rotate(45deg);
  z-index: 999;
}

.nav_tap {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  box-sizing: border-box;
  border-left: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  text-align: center;
  font-size: 30rpx;
  display: inline-block;
  background: #f5f5f5;
  position: relative;
}

.ul {
  position: relative;
}

.typeTitle {
  border-right: 1px solid #e5e5e5;
}

.titleList {
  position: absolute;
  top: -700rpx;
  left: 0;
  width: 100%;
  /* transform:translateY(100%); */
  background: white;
  text-align: left;
  z-index: 1;
  font-size: 30rpx;
  color: #888;
  /* height: 200px; */
  overflow-y: scroll;
}

.titleList view {
  border-bottom: 1px solid #e5e5e5;
  text-indent: 32rpx;
  padding: 20rpx 0;
}

.li {
  display: inline-block;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  width: 50%;
  box-sizing: border-box;
  background: #f5f5f5;
  position: relative;
  z-index: 100;
  font-size: 32rpx;
  color: #24a99b;
}

.maintenanceStatus {
  position: absolute;
  top: 0;
  font-size: 26rpx;
  width: 40rpx;
  text-align: center;
  height: 182rpx;
  color: white;
  padding-top: 14rpx;
  line-height: 32rpx;
  background: #1ba39c;
  /* box-shadow: 0rpx -10rpx 10px black */
}

.relate {
  width: 100%;
  height: 100%;
  position: relative;
}

.before {
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 30rpx;
  height: 30rpx;
  background: white;
  transform: translate(-50%, 50%) rotate(45deg);
}
