/* 添加物料领用盒子 */

.addMeterielBox {
  width: 90%;
  height: 900rpx;
  background: white;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1000000000;
  animation: addMeterielBox 0.5s;
  border-radius: 15rpx;
  overflow: hidden;
}

.addMeterielBox_head {
  width: 100%;
  height: 80rpx;
  background: #1ba39c;
  line-height: 80rpx;
  font-size: 32rpx;
}

.addMeterielBox_title {
  color: white;
  padding-left: 40rpx;
  float: left;
}

.addMeterielBox_cancel {
  float: right;
  padding-right: 30rpx;
  color: white;
  font-size: 50rpx;
  line-height: 70rpx;
}

@keyframes addMeterielBox {
  from {
    transform: translate(-50%, -500%);
  }

  to {
    transform: translate(-50%, -50%);
  }
}

