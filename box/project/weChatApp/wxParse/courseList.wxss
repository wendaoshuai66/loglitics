/* style1 样式 */

.loading_content {
  color: red;
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -80%);
  background: rgba(0, 0, 0, 0.7);
  z-index: 99999999999999999999;
}

.loadingMask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 888888;
}

.loading_round {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  /* border: 5rpx solid #fff; */
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
  /* animation: roundAnimation 1s;
  animation-iteration-count: infinite;
  animation-timing-function:linear; */
}

/* @keyframes roundAnimation{
  from{
    transform: translate(-50%, -50%) rotate(0deg);
  }
  to{
    transform: translate(-50%, -50%) rotate(360deg);
  } */

/* } */

.loading_animation_round1 {
  width: 20rpx;
  height: 20rpx;
  background: #64662e;
  border-radius: 100%;
  position: absolute;
  top: 80%;
  left: 0;
  transform: translate(-50%, -50%);
}

.loading_animation_round2 {
  width: 20rpx;
  height: 20rpx;
  background: #64662e;
  border-radius: 100%;
  position: absolute;
  top: 80%;
  right: 0;
  transform: translate(50%, -50%);
}

.loading_animation_round3 {
  width: 20rpx;
  height: 20rpx;
  background: #64662e;
  border-radius: 100%;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%, -50%);
}

.loading_text {
  color: white;
  text-align: center;
  position: absolute;
  bottom: 14%;
  left: 50%;
  transform: translate(-50%);
  font-size: 25rpx;
  width: 100%;
  /* padding-bottom: 100rpx; *//* font-weight: bold; */
}

/* 搜索框样式 start */

.TopBlock {
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  position: fixed;
  height: 45px;
  z-index: 19999;
  background: #dedede;
}

.cancel {
  position: absolute;
  top: 50%;
  right: 38rpx;
  font-size: 28rpx;
  color: #888;
  transform: translateY(-50%);
  z-index: 10000000000000000000;
   /* background: red;  */
  text-align: right;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 10px;
}

.searchInput {
  padding-left: 30px;
  font-size: 27rpx;
  line-height: 45rpx;
  height: 31px;
  width: 85%;
}

.logBox {
  display: none;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  /* height: 500rpx; */
  background: white;
  transform: translateY(100%);
  z-index: 1000000000001;
  /* display: inline-block; */
}

.logText {
  text-align: center;
  color: #dedede;
  font-size: 30rpx;
}

.logItemBox {
  padding-left: 20rpx;
  font-size: 28rpx;
  color: #999;
  border-bottom: 1px solid #e5e5e5;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
}

.logBottom {
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 14rpx 0rpx;
}

.mask {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: black;
  opacity: 0.7;
  z-index: 1000000;
  transform: translateY(100%);
}

.searchBox {
  width: 94%;
  height: 70%;
  background: white;
  position: absolute;
  border-radius: 5px;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

/* 搜索框样式 end */
